{
  "name": "bitrise-cache",
  "version": "0.0.4",
  "private": true,
  "description": "Cache artifacts using Bitrise cache backend with GitHub Actions interface compatibility",
  "main": "dist/main/index.js",
  "scripts": {
    "build": "ncc build src/main.js -o dist/main && ncc build src/post.js -o dist/post",
    "postbuild": "sed -i '' 's/@VERSION@/'\"$npm_package_version\"'/g' dist/main/index.js dist/post/index.js"
  },
  "keywords": [
    "actions",
    "cache",
    "bitrise"
  ],
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/exec": "^1.1.1",
    "@actions/io": "^1.1.3",
    "@actions/tool-cache": "^2.0.1"
  },
  "devDependencies": {
    "@vercel/ncc": "^0.38.3"
  }
}
